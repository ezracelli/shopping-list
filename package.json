{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "A shopping list web app built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "yarn --cwd client install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "yarn --cwd client start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "author": "Ezra Celli",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "mongoose": "^5.2.7"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  }
}
